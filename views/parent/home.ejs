<%include ../partials/header_parent%>
  
  <div class="container">
    <form>
      <div class="row">
        <div class="form-group col-auto">
         <label for="username">Username</label>
         <input  readonly type="text" class="form-control form-control-plaintext" id="username" placeholder="<%= parent.username %>"> </input>
        </div>
       <div class="form-group col-auto">
         <label for="mobNo">Mobile Number</label>
         <input readonly type="text" class="form-control form-control-plaintext" id="mobNo" placeholder=<%= parent.mobNo %>></input>
       </div>  
      </div>
     <div class="row">
       <div class="form-group col-auto">
         <label for="addr"> Address </label>
         <input readonly type="text" class="form-control form-control-plaintext" id="addr" placeholder="<%= parent.address %>"></input>         
       </div>
       <div class="form-group col-auto">
         <label for="email"> Email ID: </label>
         <input readonly type="text" class="form-control form-control-plaintext" id="email" placeholder="<%=parent.emailID%>"></input>
       </div> 
     </div>     
   </form>
  </div>
<%include ../partials/footer%>
  